{
  "name": "polyflow",
  "version": "1.1.0",
  "description": "",
  "private": false,
  "scripts": {
    "start": "node dist",
    "build": "tsc",
    "build:watch": "tsc -w",
    "dev": "concurrently -k \"npm run build:watch\" \"npm run start:dev\"",
    "start:prod": "NODE_ENV=production node dist",
    "start:dev": "nodemon dist",
    "test": "jest --runInBand",
    "test:watch": "jest --runInBand --watch",
    "test:cover": "jest --runInBand --collectCoverage --coverageDirectory ./coverage",
    "process-logs": "FILE=examples/Provenance/logs node examples/Provenance/processLogFile.js",
    "kepler-entity-model": "node examples/Provenance/Kepler.js",
    "swift-entity-model": "node examples/Provenance/Swift.js",
    "new-version": "semantic-release --allow-same-version",
    "format": "prettier-standard $(git diff --name-only --cached) --fix",
    "add-changed-files": "git add $(git diff --name-only --cached --diff-filter=d)"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yanmendes/polyflow.git"
  },
  "author": "Yan Mendes",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/yanmendes/polyflow/issues"
  },
  "homepage": "https://github.com/yanmendes/polyflow#readme",
  "dependencies": {
    "apollo-server-express": "^2.4.2",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "express-pino-logger": "^4.0.0",
    "graphql": "^14.1.1",
    "graphql-type-json": "^0.2.4",
    "node-fetch": "^2.6.0",
    "pg": "^7.4.3",
    "pino": "^5.11.1",
    "pino-pretty": "^2.5.0",
    "promise-mysql": "^3.3.2",
    "tagged-template-noop": "^2.1.0",
    "typeorm": "^0.2.13"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@semantic-release/git": "^7.0.17",
    "@semantic-release/npm": "^5.3.3",
    "@types/cors": "^2.8.4",
    "@types/dotenv": "^6.1.0",
    "@types/express": "^4.16.1",
    "@types/graphql-type-json": "^0.1.3",
    "@types/jest": "^24.0.15",
    "@types/node-fetch": "^2.3.7",
    "@types/pg": "^7.4.11",
    "@types/pino": "^5.8.7",
    "apollo-boost": "^0.4.3",
    "babel-core": "^7.0.0-bridge.0",
    "codecov": "^3.1.0",
    "concurrently": "^4.1.1",
    "jest": "^24.8.0",
    "nodemon": "^1.18.9",
    "pre-commit": "^1.2.2",
    "prettier-standard": "^15.0.1",
    "semantic-release": "^15.13.30",
    "semantic-release-conventional-commits": "^2.0.1",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.0.2",
    "tslint": "^5.12.1",
    "typescript": "^3.5.3"
  },
  "pre-commit": [
    "format",
    "add-changed-files"
  ],
  "release": {
    "analyzeCommits": {
      "path": "semantic-release-conventional-commits"
    },
    "plugins": [
      [
        "@semantic-release/npm",
        {
          "npmPublish": false
        }
      ],
      [
        "@semantic-release/git",
        {
          "assets": [
            "src/**/*.js",
            "package.json"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  }
}
